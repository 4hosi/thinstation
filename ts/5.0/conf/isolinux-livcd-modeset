timeout 150

DEFAULT vesamenu.c32
PROMPT 0

LABEL Back
MENU LABEL Go Back
	KERNEL vesamenu.c32
	APPEND isolinux.cfg

LABEL 800x600
MENU LABEL 800x600
	KERNEL vmlinuz
	APPEND initrd=initrd video=uvesafb:off SCREEN_RESOLUTION=800x600 splash=silent,theme:default console=tty1 loglevel=3 LM=4

