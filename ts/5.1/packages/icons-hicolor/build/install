#!/bin/sh

INSTALLDIR=./packages/icons-hicolor
EXTRADIR=$INSTALLDIR/build/extra
TMPDIR=./build-icons-hicolor

for i in hicolor-icon-theme; do
	repackage $i -t $TMPDIR
	cp -Prf $TMPDIR/$i/* $INSTALLDIR/.
	rm -rf $TMPDIR
done

if [ -e $EXTRADIR ]; then
	cp -Prfd $EXTRADIR/* $INSTALLDIR/.
fi

returnval=$?

exit $returnval
