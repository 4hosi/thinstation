# Description: Thinstation essential package
# URL: http://www.thinstation.org
# Maintainer: Donald A. Cupp Jr. (don cupp jr at ya hoo dot com)

name=loadlin
version=1.6e
release=1
source=(http://youpibouh.thefreecat.org/loadlin/loadlin-1.6e.tgz)

build() {
	cd $name-$version
	mkdir -p $PKG/ts/components/loadlin/bin
	cp loadlin.exe $PKG/ts/components/loadlin/bin/.
}
